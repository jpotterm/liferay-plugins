<?xml version="1.0"?>

<custom-sql>
	<sql id="com.liferay.portal.workflow.kaleo.service.persistence.KaleoDefinitionFinder.countByC_N_A_S">
		<![CDATA[
			SELECT
				COUNT(*) AS COUNT_VALUE
			FROM
				KaleoDefinition
			WHERE
				(KaleoDefinition.companyId = ?)
		]]>
	</sql>
	<sql id="com.liferay.portal.workflow.kaleo.service.persistence.KaleoDefinitionFinder.findByC_N_A_S">
		<![CDATA[
			SELECT
				{KaleoDefinition.*}
			FROM
				KaleoDefinition
			WHERE
				(KaleoDefinition.companyId = ?)
		]]>
	</sql>
</custom-sql>